<odoo>
    <template
    id="report_delivery_document_inherit_stock_picking_valued"
    inherit_id="stock.report_delivery_document"
  >
        <xpath expr="//th[@name='th_sm_quantity']" position="after">
            <t t-set="display_discount" t-value="any(l.discount for l in o.move_ids)" />
            <th name="th_sm_price_unit" class="text-end">Price Unit</th>
            <th
        name="th_sm_discount"
        t-if="display_discount"
        class="text-end"
      >Discount</th>
            <th name="th_sm_taxes">Taxes</th>
            <th name="th_sm_subtotal" class="text-end">Subtotal</th>
        </xpath>
        <xpath expr="//table[@name='stock_move_table']/tbody/tr/td[3]" position="after">
            <td class="text-end">
                <span t-field="move.price_unit" />
            </td>
            <td t-if="display_discount" class="text-end">
                <span t-field="move.discount" />
            </td>
            <t
        t-set="taxes"
        t-value="', '.join([(tax.invoice_label or tax.name) for tax in move.tax_id])"
      />
            <td
        name="td_taxes"
        t-attf-class="text-end {{ 'text-nowrap' if len(taxes) &lt; 10 else '' }}"
      >
                <span t-out="taxes">Tax 15%</span>
            </td>
            <td class="text-end">
                <span t-field="move.price_subtotal" />
            </td>
        </xpath>
        <xpath expr="//table[@name='stock_move_line_table']" position="after">
            <div class="clearfix" name="so_total_summary">
                <div id="total" class="row" name="total">
                    <div
            t-attf-class="#{'col-6' if report_type != 'html' else 'col-sm-7 col-md-6'} ms-auto"
          >
                        <table class="table table-sm table-borderless">
                            <t t-call="sale.document_tax_totals">
                                <t t-set="tax_totals" t-value="o.tax_totals" />
                                <t t-set="currency" t-value="o.currency_id" />
                            </t>
                        </table>
                    </div>
                </div>
            </div>
        </xpath>
        <xpath expr="//th[@name='th_sml_quantity']" position="after">
            <t t-set="display_discount" t-value="any(l.discount for l in o.move_ids)" />
            <th name="th_sm_price_unit" class="text-end">Price Unit</th>
            <th name="th_sm_discount" class="text-end">Discount</th>
            <th name="th_sm_taxes">Taxes</th>
            <th name="th_sm_subtotal" class="text-end">Subtotal</th>
        </xpath>
    </template>

    <template
    id="stock_report_delivery_aggregated_move_lines_inherit_stock_picking_valued"
    inherit_id="stock.stock_report_delivery_aggregated_move_lines"
  >
        <xpath expr="//tr/td[3]" position="after">
            <td class="text-end">
                <span t-esc="aggregated_lines[line]['price_unit']" />
            </td>
            <td class="text-end">
                <span t-esc="aggregated_lines[line]['discount']" />
            </td>
            <td>
                <span
          t-out="', '.join(map(lambda x: (x.invoice_label or x.name), aggregated_lines[line]['tax_id']))"
        />
            </td>
            <td class="text-end">
                <span t-esc="aggregated_lines[line]['price_subtotal']" />
            </td>
        </xpath>
    </template>
</odoo>
